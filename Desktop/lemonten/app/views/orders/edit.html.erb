<h1>Orders#edit</h1>

<p>Update your orders please :</p> 
<%= form_for Order.new do |f| %>

    <div class="field" >
    <%= f.label :name %><br />
     <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
   </div>
  
   <div class="field">
     <%= f.label :quntity %><br />
     <%= f.number_field :quntity %>
   </div>
  
   <div class="field">
     <%= f.label :table_number %><br />
     <%= f.number_field :table_number  %>
   </div>
  
   <div class="actions">
     <%= f.submit "Submit" %>
   </div>
    
   <% end %>



